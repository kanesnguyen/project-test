    "express-session": "^1.17.2",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "module-alias": "^2.2.2",
