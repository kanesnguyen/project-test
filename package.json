    "multer": "^1.4.3",
    "mysql2": "^2.2.5",
    "node-cron": "^3.0.0",
    "node-stream-zip": "^1.15.0",
    "node-xlsx": "^0.17.1",
    "nodemailer": "^6.6.5",
    "passport": "^0.4.1",
